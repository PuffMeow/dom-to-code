{
  "name": "dom-to-code",
  "version": "1.5.0",
  "description": "navigate to the code through the dom",
  "keywords": [
    "dom",
    "code",
    "inspector",
    "vite",
    "vite-plugin",
    "webpack",
    "webpack-plugin",
    "vue",
    "react"
  ],
  "contributors": [
    {
      "name": "tuocangyu",
      "url": "https://github.com/better-tcy"
    },
    {
      "name": "guhuatai",
      "url": "https://github.com/2214962083"
    }
  ],
  "funding": "https://github.com/sponsors/better-tcy",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "pnpm type-check &&unbuild",
    "build:watch": "pnpm build -- --stub",
    "clean": "rimraf ./dist/**/*",
    "type-check": "tsc --noEmit"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./shared-utils": {
      "types": "./dist/shared-utils.d.ts",
      "require": "./dist/shared-utils.cjs",
      "import": "./dist/shared-utils.mjs"
    },
    "./unplugin": {
      "types": "./dist/unplugin.d.ts",
      "require": "./dist/unplugin.cjs",
      "import": "./dist/unplugin.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.mjs"
    },
    "./webpack-loader": {
      "types": "./dist/webpack-loader.d.ts",
      "require": "./dist/webpack-loader.cjs",
      "import": "./dist/webpack-loader.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.mjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "require": "./dist/nuxt.cjs",
      "import": "./dist/nuxt.mjs"
    },
    "./*": "./*"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/better-tcy/dom-to-code",
    "directory": "packages/dom-to-code"
  },
  "bugs": {
    "url": "https://github.com/better-tcy/dom-to-code/issues"
  },
  "homepage": "https://github.com/better-tcy/dom-to-code#readme",
  "dependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.19.3",
    "@babel/types": "^7.19.4",
    "@rollup/pluginutils": "^5.0.2",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-dom": "^3.2.41",
    "launch-editor": "^2.6.0",
    "loader-utils": "2.0.3",
    "magic-string": "^0.26.7",
    "unplugin": "^0.10.1"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.19",
    "@types/loader-utils": "2.0.3",
    "conventional-changelog-cli": "*",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.12",
    "esno": "*",
    "express": "^4.18.2",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.3",
    "typescript": "*",
    "unbuild": "*",
    "vite": "*",
    "webpack": "^5.74.0"
  }
}
